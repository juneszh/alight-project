{
    "name": "juneszh/alight-project",
    "description": "A project template with Alight framework",
    "type": "project",
    "license": "MIT",
    "keywords": [
        "php",
        "lightweight",
        "framework",
        "restful",
        "fast-route",
        "medoo",
        "simplecache",
        "psr-16",
        "monolog",
        "whoops",
        "job-scheduler"
    ],
    "authors": [
        {
            "name": "June So",
            "email": "alight@juneszh.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "ctr\\": "app/Controllers",
            "job\\": "app/Jobs",
            "mod\\": "app/Models",
            "svc\\": "app/Services"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "juneszh/alight": "dev-main"
    },
    "prefer-stable": true,
    "scripts": {
        "admin": [
            "@composer require juneszh/alight-admin",
            ""
        ],
        "admin-build": "cd vendor/juneszh/alight-admin/npm && npm run build",
        "admin-download": ""
    },
    "config": {
        "optimize-autoloader": true
    }
}
